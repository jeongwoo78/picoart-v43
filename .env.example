# PicoArt Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Replicate API (Required)
# Get your API key from: https://replicate.com/account/api-tokens
REPLICATE_API_KEY=your_replicate_api_key_here

# Anthropic API (Required for AI artist selection)
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Environment
NODE_ENV=development

# Debug Mode (Optional)
DEBUG=false

# REMOVED - No longer needed (security improvement)
# VITE_REPLICATE_API_KEY=removed_for_security
